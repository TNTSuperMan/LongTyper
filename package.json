{
  "name": "longtyper",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:electron": "npx electron index.js",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "build:pages": "run-p build:license && run-p build:html",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:license": "node check-license.js",
    "build:electron": "run-p build:electron:win",
    "build:electron:win": "./node_modules/.bin/electron-builder --win --x64"
  },
  "dependencies": {
    "pinia": "^2.2.6",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@types/vue": "^2.0.0",
    "@types/vue-router": "^2.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "license-checker": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-singlefile": "^2.0.3",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vue-tsc": "^2.1.10"
  },
  "build": {
    "directories": {
      "output": "dist/electron"
    },
    "files": [
      "./dist/index.html",
      "index.js",
      "preload.js"
    ],
    "win": {
      "icon": "public/icon.ico",
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
